   "source": [
    "# Zadanie 1\n",
    "\n",
    "waga = 70\n",
    "wzrost = 1.75\n",
    "\n",
    "BMI = waga / (wzrost ** 2)\n",
    "\n",
    "print(\"BMI: \", round(BMI, 2))\n",
    "\n",
    "if(BMI >= 25):\n",
    "    print(\"Interpretacja: Nadwaga\")\n",
    "elif(BMI < 25 and BMI >= 18.5):\n",
    "    print(\"Interpretacja: Norma\")\n",
    "else:\n",
    "    print(\"Interpretacja: Niedowaga\")"
   ],
